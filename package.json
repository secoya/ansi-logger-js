{
	"author": "Secoya A/S <npm@secoya.dk>",
	"contributors": [
		{
			"name": "Brian K. Christensen",
			"email": "briankchristensen@gmail.com",
			"url": "https://github.com/thetrompf"
		}
	],
	"dependencies": {
		"cli-color": "1.2.0",
		"moment": "2.18.1",
		"tslib": "1.7.1"
	},
	"description": "Console logger with support for colors and log levels, it complies to the default console.log interface, with methods like log,error,warn,debug and extended with some extra levels for nice formatting purposes.",
	"devDependencies": {
		"@types/cli-color": "0.3.29",
		"@types/jest": "20.0.8",
		"@types/moment": "2.13.0",
		"@types/node": "8.0.28",
		"jest": "21.0.2",
		"prettier": "1.6.1",
		"ts-jest": "21.0.0",
		"tslint": "5.7.0",
		"tslint-secoya": "1.0.6",
		"typescript": "2.5.2"
	},
	"engines": {
		"node": ">= 4.8.4"
	},
	"jest": {
		"globals": {
			"ts-jest": {
				"tsConfigFile": "tsconfig.test.json"
			}
		},
		"moduleFileExtensions": [
			"js",
			"ts"
		],
		"testMatch": [
			"**/__tests__/*.ts"
		],
		"transform": {
			".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
		}
	},
	"keywords": [
		"log",
		"logger",
		"ansi",
		"colors"
	],
	"license": "BSD-3-Clause",
	"main": "lib/index.js",
	"name": "ansi-logger",
	"repository": "https://github.com/secoya/ansi-logger-js",
	"scripts": {
		"build": "npm run clean && tsc -p tsconfig.build.json",
		"clean": "rm -Rf lib/ types/",
		"fmt": "prettier --write -l 'src/**/*.ts'",
		"lint": "tslint src/",
		"test": "jest",
		"prepublishOnly": "npm run clean && npm test && npm run lint && npm run build"
	},
	"types": "types/index.d.ts",
	"version": "2.0.0"
}
